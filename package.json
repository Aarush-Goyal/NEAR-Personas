{
    "name": "react-parcel-boilerplate",
    "version": "1.0.0",
    "description": "React Parcel Boilerplate",
    "author": "Matt Lockyer",
    "license": "ISC",
    "dependencies": {
        "@ceramicnetwork/http-client": "^0.9.6",
        "@ceramicstudio/idx": "^0.8.0",
        "@ceramicstudio/idx-tools": "^0.8.0",
        "@material-ui/core": "^4.11.3",
        "@material-ui/icons": "^4.11.2",
        "3id-did-provider": "^0.5.1",
        "animejs": "^3.2.1",
        "copy-to-clipboard": "^3.3.1",
        "ipfs-api": "^26.1.2",
        "near-api-js": "^0.38.0",
        "near-seed-phrase": "^0.1.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-hook-form": "^6.15.4",
        "react-quill": "^1.3.5",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/preset-react": "^7.12.10",
        "babel-plugin-react-html-attrs": "^3.0.5",
        "eslint": "^7.14.0",
        "gh-pages": "^3.1.0",
        "parcel": "^2.0.0-beta.1",
        "rimraf": "^3.0.2"
    },
    "resolutions": {
        "multicodec": "2.0.0"
    },
    "browserslist": [
        "last 2 Chrome versions"
    ],
    "scripts": {
        "deploy:testnet": "npm run build && gh-pages -d dist",
        "preinstall": "npx npm-force-resolutions",
        "deploy": "REACT_APP_ENV=\"prod\" yarn build && cp -a static/* dist && gh-pages -d dist",
        "prebuild": "npm run clean",
        "build": "parcel build src/index.html --no-source-maps --public-url ./",
        "clean": "rimraf ./dist && rimraf .parcel-cache",
        "fix": "eslint src/ --fix",
        "start": "npm run prebuild && parcel src/index.html --open"
    }
}
